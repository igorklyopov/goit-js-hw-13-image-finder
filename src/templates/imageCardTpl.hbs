{{#each this.hits}}
<li class="gallary-item">
  <div class="image-wrap">
    <img class="gallary-image" src="{{webformatURL}}" data-source={{largeImageURL}} alt="{{tags}}" />
  </div>

  <div class="gallary-item-stats">
    <p class="stats-item">
      <i class="material-icons basic-castom-style">thumb_up</i>
      {{likes}}
    </p>
    <p class="stats-item">
      <i class="material-icons basic-castom-style">visibility</i>
      {{views}}
    </p>
    <p class="stats-item">
      <i class="material-icons basic-castom-style">comment</i>
      {{comments}}
    </p>
    <p class="stats-item">
      <i class="material-icons basic-castom-style">cloud_download</i>
      {{downloads}}
    </p>
  </div>
</li>
{{/each}}